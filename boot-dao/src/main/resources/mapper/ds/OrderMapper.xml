<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.boot.dao.ds.OrderMapper">
    <select id="selectByUserId" resultType="com.boot.domain.ds.Order">
        selectI
        order_id as orderId,
        user_id as userId,
        status as status
        from t_order
    </select>

</mapper>
